{
  "name": "age-calculator",
  "version": "2.0.1",
  "description": "Age Calculator and Updater as Chrome Extension by Rofi",
  "sideEffects": false,
  "scripts": {
    "prebuild": "node generate-build-version",
    "start": "yarn prebuild && NODE_ENV=development webpack serve --config config/webpack.config.js --env env=dev --host 0.0.0.0",
    "build:base": "NODE_ENV=production webpack --config config/webpack.config.js --env env=prod",
    "build": "yarn build:base",
    "build:extension": "yarn prebuild && BUILD_ENV=extension yarn build:base && node after-build",
    "lint-fix": "eslint . --ext .js,.jsx -f table --fix; exit 0"
  },
  "keywords": [
    "chrome",
    "extension",
    "age-calculator",
    "calculate-your-age"
  ],
  "author": "Rofi",
  "license": "MIT",
  "dependencies": {
    "@date-io/core": "^1.3.6",
    "@date-io/dayjs": "^1.3.13",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.2.10",
    "dayjs": "^1.9.8",
    "goods-core": "^1.6.15",
    "goods-ui": "^1.10.10",
    "immer": "^9.0.6",
    "react": "^17.0.1",
    "react-color": "^2.19.3",
    "react-confetti": "^6.0.0",
    "react-dom": "^17.0.1",
    "react-fast-compare": "^3.2.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/chrome": "0.0.127",
    "@types/node": "^14.14.17",
    "@types/react": "^17.0.0",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "archiver": "^5.1.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-react-app": "^10.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.2",
    "copy-webpack-plugin": "^7.0.0",
    "core-js": "^3.8.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.4.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "markdown-loader": "^5.1.0",
    "preload-webpack-plugin": "^3.0.0-beta.4",
    "prettier": "^2.2.1",
    "terser-webpack-plugin": "^1.4.5",
    "ts-loader": "^8.0.13",
    "typescript": "^4.5.5",
    "webpack": "^5.11.1",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.0",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.7.3",
    "workbox-webpack-plugin": "^6.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint -f table"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
