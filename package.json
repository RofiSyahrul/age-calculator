{
  "author": "Rofi",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "dayjs": "1.11.7",
    "immer": "9.0.16",
    "next": "13.0.6",
    "react": "18.2.0",
    "react-color": "2.19.3",
    "react-confetti": "6.1.0",
    "react-date-picker": "9.1.0",
    "react-dom": "18.2.0",
    "react-fast-compare": "3.2.0",
    "styled-components": "5.3.6",
    "zustand": "4.1.5"
  },
  "description": "Age Calculator and Updater as Chrome Extension by Rofi",
  "devDependencies": {
    "@babel/eslint-parser": "7.19.1",
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@next/bundle-analyzer": "13.0.6",
    "@supabase/supabase-js": "2.1.3",
    "@types/archiver": "5.3.1",
    "@types/chrome": "0.0.204",
    "@types/dotenv": "8.2.0",
    "@types/node": "18.11.13",
    "@types/react": "18.0.26",
    "@types/react-color": "3.0.6",
    "@types/react-dom": "18.0.9",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "archiver": "5.3.1",
    "core-js": "3.26.1",
    "dotenv": "16.0.3",
    "eslint": "8.29.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-formatter-table": "7.32.1",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "next-compose-plugins": "2.2.1",
    "next-pwa": "5.6.0",
    "prettier": "2.8.1",
    "standard-version": "9.5.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "homepage": "https://age.rofi.link",
  "keywords": [
    "chrome",
    "extension",
    "age-calculator",
    "calculate-your-age"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint -f table"
    ]
  },
  "name": "age-calculator",
  "repository": {
    "type": "git",
    "url": "https://github.com/RofiSyahrul/age-calculator.git"
  },
  "scripts": {
    "after-build": "TS_NODE_PROJECT=tsconfig.node.json ts-node scripts/after-build",
    "analyze": "ANALYZE=true yarn build",
    "build": "next build && next export",
    "build:extension": "yarn prebuild && BUILD_ENV=extension next build && next export && yarn after-build",
    "clean": "rm -rf .next out",
    "dev": "PORT=8045 next dev",
    "generate-manifest": "TS_NODE_PROJECT=tsconfig.node.json ts-node scripts/generate-manifest",
    "lint-fix": "eslint . --ext .js,.jsx,.tsx,.ts -f table --fix; exit 0",
    "prebuild": "yarn generate-manifest",
    "predev": "yarn generate-manifest",
    "prepare": "husky install",
    "release": "standard-version --no-verify && yarn build:extension && git add . && git commit -m \"chore: update archived build to v$(jq -r .version package.json)\" --no-verify",
    "start": "next start"
  },
  "sideEffects": false,
  "version": "3.0.0"
}
