{
  "name": "age-calculator",
  "version": "2.0.1",
  "description": "Age Calculator and Updater as Chrome Extension by Rofi",
  "sideEffects": false,
  "scripts": {
    "prepare": "husky install",
    "prebuild": "node generate-build-version",
    "start": "yarn prebuild && NODE_ENV=development webpack serve --config config/webpack.config.js --env env=dev --host 0.0.0.0",
    "build:base": "NODE_ENV=production webpack --config config/webpack.config.js --env env=prod",
    "build": "yarn build:base",
    "build:extension": "yarn prebuild && BUILD_ENV=extension yarn build:base && node after-build",
    "lint-fix": "eslint . --ext .js,.jsx -f table --fix; exit 0"
  },
  "keywords": [
    "chrome",
    "extension",
    "age-calculator",
    "calculate-your-age"
  ],
  "author": "Rofi",
  "license": "MIT",
  "dependencies": {
    "@date-io/core": "^1.3.6",
    "@date-io/dayjs": "^1.3.13",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.2.10",
    "dayjs": "^1.9.8",
    "goods-core": "^1.6.15",
    "goods-ui": "^1.10.10",
    "immer": "^9.0.12",
    "react": "^17.0.1",
    "react-color": "^2.19.3",
    "react-confetti": "^6.0.0",
    "react-dom": "^17.0.1",
    "react-fast-compare": "^3.2.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/chrome": "^0.0.177",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.38",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.21",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "archiver": "^5.3.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-react-app": "^10.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.2.4",
    "copy-webpack-plugin": "^10.2.4",
    "core-js": "^3.20.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-formatter-table": "^7.32.1",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.2",
    "preload-webpack-plugin": "^3.0.0-beta.4",
    "prettier": "^2.5.1",
    "terser-webpack-plugin": "^1.4.5",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^5.8.0",
    "workbox-webpack-plugin": "^6.4.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint -f table"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
