{
  "author": "Rofi",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "dayjs": "^1.9.8",
    "immer": "^9.0.12",
    "next": "^12.1.0",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-confetti": "^6.0.0",
    "react-date-picker": "^8.4.0",
    "react-dom": "^17.0.2",
    "react-fast-compare": "^3.2.0",
    "styled-components": "^5.2.1"
  },
  "description": "Age Calculator and Updater as Chrome Extension by Rofi",
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@next/bundle-analyzer": "^12.1.0",
    "@supabase/supabase-js": "^1.30.0",
    "@types/archiver": "^5.3.1",
    "@types/chrome": "^0.0.177",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^17.0.15",
    "@types/react": "^17.0.39",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.22",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "archiver": "^5.3.0",
    "commitizen": "^4.2.4",
    "core-js": "^3.20.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-formatter-table": "^7.32.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.2",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.4.4",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "homepage": "https://age.rofi.link",
  "keywords": [
    "chrome",
    "extension",
    "age-calculator",
    "calculate-your-age"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint -f table"
    ]
  },
  "name": "age-calculator",
  "repository": {
    "type": "git",
    "url": "https://github.com/RofiSyahrul/age-calculator.git"
  },
  "scripts": {
    "after-build": "TS_NODE_PROJECT=tsconfig.node.json ts-node scripts/after-build",
    "analyze": "ANALYZE=true yarn build",
    "build": "next build && next export",
    "build:extension": "yarn prebuild && BUILD_ENV=extension next build && next export && yarn after-build",
    "clean": "rm -rf .next out",
    "dev": "PORT=8045 next dev",
    "generate-manifest": "TS_NODE_PROJECT=tsconfig.node.json ts-node scripts/generate-manifest",
    "lint-fix": "eslint . --ext .js,.jsx,.tsx,.ts -f table --fix; exit 0",
    "prebuild": "yarn generate-manifest",
    "predev": "yarn generate-manifest",
    "prepare": "husky install",
    "release": "standard-version --no-verify && yarn build:extension && git add . && git commit -m \"chore: update archived build to v$(jq -r .version package.json)\" --no-verify",
    "start": "next start"
  },
  "sideEffects": false,
  "version": "2.4.2"
}
