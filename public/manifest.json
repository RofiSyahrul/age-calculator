{
  "short_name": "Chrome Extension",
  "name": "Chrome Extension by Rofi",
  "version": "1.1.3",
  "description": "Build an Extension using react!",
  "manifest_version": 2,
  "icons": ["assets/manifest-icon-192.png", "assets/manifest-icon-512.png"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'sha256-FyiKpy3PpN1/MM6lBhmjdt7P5PXAcmjHEWQr2bhYhkk=''; object-src 'self'; default-src 'self' 'unsafe-inline'"
}